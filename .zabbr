abbr -="cd -"
abbr brewup="brew update && brew upgrade"
abbr dotfl="cd \$DOTFILES.local"

abbr gad="git add"
abbr gbn="git rev-parse --abbrev-ref HEAD"
abbr gcd1="git clone --depth 1 https://github.com/"
abbr gcl="git clean"
abbr gclone="git clone git@github.com:mattmc3/"
abbr gcmt="git commit -am "
abbr gco="git checkout"
abbr gcob="git checkout -b "
abbr gcod="git checkout develop"
abbr gcom="git checkout main"
abbr get="git"
abbr glg="git log"
abbr glog="git log --graph --pretty='%Cred%h%Creset -%C(auto)%d%Creset %s %Cgreen(%ad) %C(bold blue)<%an>%Creset' --date=short"
abbr gpll="git pull"
abbr gpristine="git reset --hard && git clean -fdx"
abbr gpsh="git push"
abbr gpsuo="git push --set-upstream origin (git rev-parse --abbrev-ref HEAD)"
abbr grv="git remote -v"
abbr gsh="git stash"
abbr gst="git status -sb"

abbr la="ls -la"
abbr ldot="ls -ld .*"
abbr ll="ls -lGh"
abbr lsa="ls -aG"
abbr secrets="cd \$XDG_DATA_HOME/secrets"
abbr tarls="tar -tvf"
abbr untar="tar -xv"
abbr zdot="cd \$ZDOTDIR"
abbr zz="exit"
